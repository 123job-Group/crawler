{
    "name": "nggiahao/crawler",
    "description": "",
    "keywords": [
        "nggiahao",
        "crawler"
    ],
    "homepage": "https://github.com/nggiahao/crawler",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Nguyen Gia Hao",
            "email": "giahao9899@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "illuminate/support": "^7.0",
        "illuminate/database": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Nggiahao\\Crawler\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nggiahao\\Crawler\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nggiahao\\Crawler\\CrawlerServiceProvider"
            ],
            "aliases": {
                "Crawler": "Nggiahao\\Crawler\\CrawlerFacade"
            }
        }
    }
}
